/* UI/search-edittext/index.wxss */

.searchContainer{
  /* 如果这个叫mContainer在引用时重名会引发覆盖 */
  display: flex;
  flex-direction: row;
}
.searchEdt{
  box-sizing: border-box;/*让padding算在box*/
  padding-left: 20px;
  width: 302px;
  height: 42px;
  background: white;
  border-bottom-left-radius: 8px;
  border-top-left-radius: 8px;
}
.searchBox{
  width: 42px;
  height: 42px;
  background:#01826E ;
  display: flex;
  align-items: center;
  justify-content: center;
  border-top-right-radius: 8px;
  border-bottom-right-radius: 8px;
}
.searchIcon{
  width: 27px;
  height: 27px;
}